{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "appurl": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "web",
        "mobile"
      ],
      "required": true,
      "default": "web"
    },
    "description": {
      "type": "richtext",
      "default": "description",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "repourl": {
      "type": "string"
    }
  }
}
